<div class="container">
  <div class="row">
    <div class="col-8 offset-2">
      <div class="card mt-4">
        <div class="card-body">
          <h5 class="card-title">Process detail</h5>
          <p>
            <strong>ID :</strong><strong id="id">{{process.id}}</strong></p>
          <p>
            <strong>Process_name</strong>: {{process.Process_name}}</p>
          <p>
            <strong>Description:</strong> {{process.Process_Description}}</p>
          <p><strong>Device 1:</strong> {{process.Device1_name}}   Type:<b id="type1">{{process.Device1_type}}</b>  Serie:<b id="serial1">{{process.IdDevice1}}</b></p>
            <div id="div1"></div>
          <p>  <strong>Device 2:</strong> {{process.Device2_name}}   Type:<b id="type2">{{process.Device2_type}}</b>  Serie:<b id="serial2">{{process.IdDevice2}}
            <div id="div2"></div>
          <p>  <strong>Device 3:</strong> {{process.Device3_name}}   Type:<b id="type3">{{process.Device3_type}}</b>  Serie:<b id="serial3">{{process.IdDevice3}}
            <div id="div3"></div>
          <p>  <strong>Device 4:</strong> {{process.Device4_name}}   Type:<b id="type4">{{process.Device4_type}}</b>  Serie:<b id="serial4">{{process.IdDevice4}}
            <div id="div4"></div>
          <p>  <strong>Device 5:</strong> {{process.Device5_name}}   Type:<b id="type5">{{process.Device5_type}}</b>  Serie:<b id="serial5">{{process.IdDevice5}}
            <div id="div5"></div>
            <a href="/">‚Üê Back To Home</a>
        </div>
      </div>
    </div>
  </div>
</div>
<script type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.2.1/jquery.min.js"></script>
<script src="/js/process.js"></script>
